<!doctype html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="en">        <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="en">               <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{{ title }}</title>
    <link rel="shortcut icon" href="{{{staticRoot}}}/images/favicon.ico" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <link rel="stylesheet" href="{{{staticRoot}}}/css/app.css">
    {{#navColor}}
    <style>
        .navbar-default {
            background-color: {{{navColor}}};
        }
    </style>
    {{/navColor}}
    <script>
        window.config = {
            appRoot: "{{{appRoot}}}",
            apiRoot: localStorage.getItem("apiRootOverride") || "{{{apiRoot}}}",
            title: "{{{title}}}",
            mesosLogPort: {{{mesosLogsPort}}},
            {{#mesosLogsPortHttps}}
            mesosLogsPortHttps: {{{mesosLogsPortHttps}}}
            {{/mesosLogsPortHttps}}
        };
    </script>
    <script src="{{{staticRoot}}}/js/app.js"></script>
    <script>require('initialize');</script>
</head>
<body>
    <div class="page-loader fixed"></div>
    <div id="page" class="container-fluid"></div>
    <div style="-webkit-transform: translateZ(0)"></div>
</body>
</html>
