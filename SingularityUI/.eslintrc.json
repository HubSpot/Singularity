{
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "extends": "hubspot/experimental",
  "globals": {
    "$": true,
    "jQuery": true,
    "_": true,
    "config": true,
    "app": true,
    "Promise": true
  },
  "rules": {
    "id-length": [2, {"min": 3, "exceptions": ["_", "id", "ui", "s3"]}]
  }
}
